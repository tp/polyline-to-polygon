{
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": false,
    "baseUrl": ".",
    "declaration": false,
    "experimentalDecorators": true,
    "importHelpers": true,
    "inlineSourceMap": false,
    "jsx": "react",
    "lib": ["dom", "es2015"],
    "module": "commonjs",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "_compiled",
    "preserveConstEnums": true,
    "removeComments": true,
    "rootDir": ".",
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es6",
    "traceResolution": false
  }
}
